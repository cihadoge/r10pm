{
  "name": "r10 Bildirimler",
  "version": "2.0",
  "manifest_version": 1,
  "description": "R10 Bildirimleri",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
	  "default_title": "r10 Bildirimler",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs", "*://*.r10.net/*", "notifications"
  ]
}
